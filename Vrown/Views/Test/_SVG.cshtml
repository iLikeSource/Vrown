@using SVGGenerator
@model Vrown.ViewModels.SubModels.SvgBlock

@{
    var model = Model;
    var svgText = Generator.dump(model.Contents);
}
<svg id="static-svg" width="@(model.Width)" height="@(model.Height)" viewBox="0 0 400 400" version="1.1" xmlns="http://www.w3.org/2000/svg">
    <rect x="0" y="0" width="400" height="400" stroke-width="5" stroke="gray" fill="white"/>
    @Html.Raw(svgText)
</svg>
